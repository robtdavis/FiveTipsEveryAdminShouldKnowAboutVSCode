{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Deploy using Manifest (sf)",
        "type": "shell",
        "command": "sf project deploy start --manifest manifest/package.xml --target-org FiveTips",
        "options": {
          "env": {
            "sfDefaultUsername": "${config:sf.defaultusername}"
          }
        },
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": []
      }
    ]
  }
  