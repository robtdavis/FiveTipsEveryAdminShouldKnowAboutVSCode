[{"cellType":"code","source":["// List all scheduled jobs ordered by next fire time","List<CronTrigger> scheduledJobs = [","    SELECT Id, CronJobDetail.Name, CronJobDetail.JobType, ","            NextFireTime, State, PreviousFireTime","    FROM CronTrigger ","    ORDER BY NextFireTime ASC NULLS LAST","    LIMIT 100","];","","System.debug('=== Scheduled Jobs Summary ===');","System.debug('Total Jobs: ' + scheduledJobs.size());","System.debug('');","","for (CronTrigger ct : scheduledJobs) {","    System.debug('Job: ' + ct.CronJobDetail.Name);","    System.debug('  Type: ' + ct.CronJobDetail.JobType);","    System.debug('  State: ' + ct.State);","    System.debug('  Next Fire: ' + ct.NextFireTime);","    System.debug('  Previous Fire: ' + ct.PreviousFireTime);","    System.debug('---');","}"],"language":"apex-anon"},{"cellType":"code","source":["","// Inactive users who own Accounts","AggregateResult[] accOwners = [","    SELECT OwnerId oid, Owner.Name name, COUNT(Id) c","    FROM Account","    WHERE Owner.IsActive = false","    GROUP BY OwnerId, Owner.Name","    ORDER BY COUNT(Id) DESC","    LIMIT 50","];","System.debug(accOwners);","","// Inactive users who own Opportunities","AggregateResult[] oppOwners = [","    SELECT OwnerId oid, Owner.Name name, COUNT(Id) c","    FROM Opportunity","    WHERE Owner.IsActive = false","    GROUP BY OwnerId, Owner.Name","    ORDER BY COUNT(Id) DESC","    LIMIT 50","];","System.debug(oppOwners);"],"language":"apex-anon"},{"cellType":"code","source":["// Failing Scheduled Jobs or Async Job failures Last 30 days","","DateTime sinceDt = System.now().addDays(-30);","","List<AsyncApexJob> failedJobs = [","    SELECT Id, ApexClass.Name, JobType, Status, NumberOfErrors, CreatedDate, CompletedDate, ExtendedStatus","    FROM AsyncApexJob","    WHERE CreatedDate >= :sinceDt","    AND Status IN ('Failed','Aborted')","    ORDER BY CreatedDate DESC","    LIMIT 200","];","System.debug(failedJobs);","","// Scheduled Apex jobs (what's currently scheduled)","List<CronTrigger> cronJobs = [","    SELECT Id, CronJobDetail.Name, CronJobDetail.JobType, State, NextFireTime, PreviousFireTime","    FROM CronTrigger","    ORDER BY NextFireTime ASC","    LIMIT 200","];","System.debug(cronJobs);"],"language":"apex-anon"},{"cellType":"code","source":["List<Contact> orphans = [","    SELECT Id, Name, Email, Phone, Owner.Name, CreatedDate, LastModifiedDate","    FROM Contact","    WHERE AccountId = null","    ORDER BY LastModifiedDate DESC","    LIMIT 200","];","","System.debug('Orphan Contacts (no Account): ' + orphans.size());","System.debug(orphans);"],"language":"apex-anon"}]